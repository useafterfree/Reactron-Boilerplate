{
  extends: 'airbnb',
  env: {
    es6: true,
    node: true,
    browser: true
  },
  globals: {
    JSONEditor: true,
    AWSCognito: true,
    AmazonCognitoIdentity: true,
    firebaseServiceAccount: true,
    $: true,
    cmsOriginalParams: true,
    _: true,
    moment: true
  },
  rules: {
    'max-statements': 0,
    'array-callback-return': 0,
    'arrow-body-style': 0,
    'arrow-parens': ['error', 'always'],
    'block-scoped-var': 0,
    'class-methods-use-this': 0,
    'comma-dangle': ["error", {
      arrays: 'never',
      objects: 'never',
      imports: 'never',
      exports: 'never',
      functions: 'ignore'
    }],
    'no-underscore-dangle': 0,
    'no-new': 0,
    'no-console': 'off',
    'consistent-return': 0,
    'func-names': 0,
    'guard-for-in': 0,
    'no-restricted-syntax': 0,
    'default-case': 0,
    'import/first': 0,
    'import/prefer-default-export': 0,
    'prefer-arrow-callback': 0,
    'max-len': [2, 200],
    'max-params': ['error', 6],
    'no-extra-bind': 0,
    'no-useless-escape': 0,
    'no-case-declarations': 0,
    'no-inline-comments': 0,
    'no-nested-ternary': 0,
    'no-param-reassign': 0,
    'no-plusplus': 0,
    'no-shadow': 0,
    'no-unused-expressions': 0,
    'one-var': 0,
    'one-var-declaration-per-line': 0,
    'prefer-destructuring': 0,
    'prefer-promise-reject-errors': 0,
    'radix': 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}
